{
  "name": "@crowdstrike/aidr-openai",
  "version": "0.0.0",
  "description": "A wrapper around the OpenAI TypeScript library that wraps the Responses API with CrowdStrike AIDR",
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CrowdStrike/aidr-openai-node.git"
  },
  "type": "commonjs",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.cts",
  "module": "./dist/index.mjs",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist",
    "LICENSE.txt",
    "README.md"
  ],
  "engines": {
    "node": "22 || >=24"
  },
  "scripts": {
    "attw": "attw --pack",
    "publint": "publint --strict",
    "build": "tsdown && pnpm typecheck",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm build"
  },
  "dependencies": {
    "@crowdstrike/aidr": "^1.0.2",
    "openai": "^6.10.0"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "0.18.2",
    "@biomejs/biome": "2.3.8",
    "@containerbase/semantic-release-pnpm": "1.3.9",
    "@tsconfig/node22": "22.0.5",
    "@types/node": "22.19.2",
    "conventional-changelog-conventionalcommits": "9.1.0",
    "lefthook": "2.0.11",
    "publint": "0.3.16",
    "semantic-release": "25.0.2",
    "tsdown": "0.17.2",
    "typescript": "5.9.3"
  }
}
